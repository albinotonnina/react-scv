    "start": "scv start --port <%= data.port %>",
    "test": "scv test"
  },
  "keywords": [],
  "devDependencies": {
    "workshare-scv": "<%= data.version %>"
  },
  "dependencies": {
    "react": "15.3.0",
    "react-addons-test-utils": "15.3.0",
    "react-dom": "15.3.0"
  }
}
