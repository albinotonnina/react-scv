{
  "name": "<%= data.packageName %>",
  "private": true,
  "version": "1.0.0",
  "description": "<%= data.description %>",
  "license": "MPL-2.0",
  "main": "./build/bundle.js",
  "config": {
    "appBuildEntry": "src/app.js",
	"umdBuildEntry": "src/module/index.js",
	"html": {
      "title": "<%= data.appTitle %>",
      "description": "<%= data.description %>"
    }
